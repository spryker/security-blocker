<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="AuthContext">
        <property name="type" type="string"/>
        <property name="ip" type="string"/>
        <property name="account" type="string"/>
    </transfer>

    <transfer name="AuthResponse">
        <property name="isSuccessful" type="bool"/>
        <property name="count" type="int"/>
        <property name="authContext" type="AuthContext"/>
    </transfer>

    <transfer name="SecurityConfigurationSetting">
        <property name="count" type="int"/>
        <property name="ttl" type="int"/>
    </transfer>

    <transfer name="RedisConfiguration">
        <property name="dataSourceNames" type="array" singular="dataSourceNames"/>
    </transfer>

    <transfer name="RedisCredentials">
        <property name="protocol" type="string"/>
    </transfer>

</transfers>
